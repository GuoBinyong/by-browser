{
  "name": "by-browser",
  "version": "1.0.7",
  "description": "by-browser 包含了 es-expand、dom-expand、bom-expand、com-tools、bro-tools 等，提供了浏览器中原生类型功能的扩展 和 一些实用的工具函数",
  "main": "dist/by-browser.cjs.js",
  "module": "dist/by-browser.esm.js",
  "browser": "dist/by-browser.umd.js",
  "types": "dist/index.d.ts",
  "sideEffects": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/GuoBinyong/by-browser"
  },
  "keywords": [
    "扩展",
    "工具",
    "JS扩展",
    "JS工具",
    "JavaScript扩展",
    "JavaScript工具",
    "expand",
    "tools",
    "JS expand",
    "JS tools",
    "JavaScript expand",
    "JavaScript tools",
    "es-expand",
    "dom-expand",
    "bom-expand",
    "com-tools",
    "bro-tools"
  ],
  "author": "郭斌勇",
  "license": "ISC",
  "files": [
    "dist/[^.]*",
    "types/[^.]*"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "node test/test.ts",
    "pretest": "npm run build"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "rollup": "^2.7.2",
    "rollup-plugin-typescript2": "^0.27.0",
    "ts-node": "^8.9.1",
    "tslib": "^1.11.1",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "bom-expand": "^1.0.6",
    "bro-tools": "^1.0.6",
    "com-tools": "^1.0.9",
    "dom-expand": "^1.0.3",
    "es-expand": "^1.0.11"
  }
}
